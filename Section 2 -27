public class App {

	public static void main(String[] args) {
		
		byte value0 = 20;
		short value1 = 55;
		int value2 = 868;
		long value3 = 235555;
		
		float value4 = 88.34f;
		double value5 = 32.4;
		
		System.out.println(Byte.MAX_VALUE);
		
		value2 = (int)value3;
		
		value5 = value5; //not good practice
		
		value2 = (int) value4;
		
		//takeaway: be careful in casting a bigger value into a type that cannot contain it
	}

}
